{
    "name": "atlas-wong/laravel-log-viewer",
    "description": "A Laravel log reader",
    "license": "MIT",
    "keywords": ["log", "log-reader", "log-viewer", "logging", "laravel", "lumen"],
    "authors": [
        {
            "name": "Atlas Wong",
            "email": "atlas.wong.times@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*|5.*"
    },
    "autoload": {
        "classmap": [
            "src/controllers"
        ],
        "psr-0": {
            "AtlasWong\\LaravelLogViewer\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "AtlasWong\\LaravelLogViewer\\LaravelLogViewerServiceProvider"
            ]
        }
    }
}
